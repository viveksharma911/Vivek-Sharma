public class palidrome {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
	     
	     int n = sc.nextInt();
	     int x = n;
	     int r=0;
	     int rev=0;
	     while(n>0){
	       r=n%10;
	       rev=(rev*10)+r;
	       n=n/10;
	     }
	     if(rev==x)
	       System.out.print("number is palindrome");
	     else
	       System.out.print("number is not palindrome");
	}

}
